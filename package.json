{
	"name": "@notwoods/logo",
	"version": "1.1.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./Logo.svelte": {
			"types": "./dist/Logo.svelte.d.ts",
			"svelte": "./dist/Logo.svelte"
		},
		"./svg/*.svg": "./svg/*.svg"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"svg/*.svg"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0-next.0"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^8.3.4",
		"@storybook/addon-interactions": "^8.3.4",
		"@storybook/addon-svelte-csf": "^5.0.0-next.3",
		"@storybook/svelte": "^8.3.4",
		"@storybook/sveltekit": "^8.3.4",
		"@storybook/test": "^8.3.4",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.1.9",
		"storybook": "^8.0.9",
		"svelte": "^5.0.0-next.0",
		"svelte-check": "^4.0.0",
		"svgo": "^3.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"vite": "^5.4.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"packageManager": "pnpm@9.0.6+sha512.f6d863130973207cb7a336d6b439a242a26ac8068077df530d6a86069419853dc1ffe64029ec594a9c505a3a410d19643c870aba6776330f5cfddcf10a9c1617"
}
